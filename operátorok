gondolt_szám = 4
print(type(11))
tipp = input('Gondoltam egy számra. Tippeld meg! ')
tipp = int(tipp)
if tipp == gondolt_szám:
    print('Ügyes vagy!')
if tipp > gondolt_szám:
    print('A tipp nagyobb mint a gondolt szám')
if tipp < gondolt_szám:
    print('A tipp kisseb mint a gondolt szám')



print('Pápá.')
